<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="AP_PIB">
    <test name="AP PIB" preserve-order="true">
        <groups>
            <run>
                <!-- exclude name="ignore"/ -->
                <include name="Acceptance"/>
                <include name="GAT"/>
            </run>
        </groups>
        <classes>
            <class name="com.ericsson.oss.ap.pib.test.cases.APHealthCheck"/>
            <class name="com.ericsson.oss.ap.pib.test.cases.APUpgrade"/>
        </classes>
    </test>
</suite>
